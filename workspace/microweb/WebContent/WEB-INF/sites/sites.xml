<?xml version="1.0" encoding="UTF-8"?>
<sites-config>
	<sites>
		<!-- <site location="admin" config="config.xml"/>  -->
		<site location="prep" config="config.xml"/>
	</sites>
	<domains>
		<!-- example canonical domains for all sites -->
		<canonical name="prep.microweb.com" site="prep"/>
		<!-- <canonical name="admin.microweb.com" site="admin"/>  -->
		
		<!-- example alias domain for a hosted site -->
		<alias name="prep2.microweb.com" site="prep"/>
		
		<!-- example redirection for a hosted site -->
		<redirect name="prep3.microweb.com" site="prep" scheme="preserve" port="preserve" uri="preserve" queryString="preserve"/>
	</domains>
</sites-config>